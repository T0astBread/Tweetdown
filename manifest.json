{
    "manifest_version": 2,
  
    "name": "Tweetdown",
    "description": "Simple Markdown in tweets",
    "version": "0.1",
  
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["formatter.js"],
            "css": ["mdstyles.css"]
        }
    ]
  }
